# MySQL 配置环境变量

# MySQL Root 密码
MYSQL_ROOT_PASSWORD=123456

# 默认创建的数据库名
MYSQL_DATABASE=myapp

# 应用用户配置
MYSQL_USER=appuser
MYSQL_PASSWORD=apppass

# 端口配置
MYSQL_PORT=3306
MYSQL_SLAVE_PORT=3307

# 服务器 ID（集群模式使用）
MYSQL_MASTER_SERVER_ID=1
MYSQL_SLAVE_SERVER_ID=2

# 复制用户配置（集群模式）
MYSQL_REPLICATION_USER=replicator
MYSQL_REPLICATION_PASSWORD=replicator_pass
